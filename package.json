    "@nuxtjs/eslint-config-typescript": "^1.0.0",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@types/aws-lambda": "^8.10.51",
    "@types/html-to-text": "^5.1.0",
    "babel-eslint": "^10.0.1",
    "enabler": "^2.9.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.2",
    "npm-run-all": "^4.1.5",
