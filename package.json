    "@nuxtjs/proxy": "^1.3.3",
    "@patarapolw/qsearch": "^1.2.2",
    "@types/js-yaml": "^3.12.4",
    "@types/mongodb": "^3.5.20",
    "dayjs": "^1.8.27",
    "dotenv-webpack": "^1.8.0",
    "fast-glob": "^3.2.2",
    "highlight.js": "^10.0.3",
    "highlightjs-vue": "github:patarapolw/highlightjs-vue",
    "html-to-text": "^5.1.1",
    "js-yaml": "^3.14.0",
    "mongodb": "^3.5.8",
    "netlify-lambda": "^1.6.3",
    "nuxt-buefy": "^0.3.2",
    "nuxt-edge": "^2.13.0-26511862.a544fc72",
