{
  "name": "nuxt-blog-template",
  "version": "0.1.0",
  "description": "A blog template using Nuxt.js as well as make contents as private as possible",
  "author": "Pacharapol Withayasakpunt",
  "private": true,
  "scripts": {
    "preinstall": "node -e \"if(process.env.npm_execpath.indexOf('yarn') === -1) throw new Error('You must use Yarn to install, not NPM')\"",
    "generate-db": "ts-node -O '{\"module\":\"commonjs\",\"noImplicitAny\":false}' scripts/build.ts",
    "dev": "run-p 'dev:**'",
    "dev:nuxt": "nuxt-ts",
    "dev:lambda": "netlify-lambda serve lambda",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "clean": "run-p 'clean:**'",
    "clean:content": "rimraf content",
    "clean:db": "ts-node -O '{\"module\":\"commonjs\",\"noImplicitAny\":false}' scripts/clean.ts",
    "build": "run-p 'build:**'",
    "build:nuxt": "nuxt-ts build && nuxt-ts export",
    "build:lambda": "netlify-lambda build lambda",
    "postinstall": "netlify-lambda install",
    "deploy": "netlify deploy --dir dist --functions lambda-dist"
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^0.4.10",
    "@nuxtjs/axios": "^5.10.3",
    "@patarapolw/make-html-frontend-functions": "^1.0.0-pre14",
    "@patarapolw/make-html-functions": "^1.0.0-pre9",
    "dayjs": "^1.8.28",
    "dotenv": "^8.2.0",
    "lunr": "^2.3.8",
    "netlify-lambda": "^1.6.3",
    "nuxt": "^2.13.0",
    "nuxt-mq": "^2.0.1",
    "zod": "^1.6.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/vue-fontawesome": "^0.1.9",
    "@nuxt/typescript-build": "^0.6.0",
    "@nuxtjs/eslint-config-typescript": "^1.0.0",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@types/aws-lambda": "^8.10.56",
    "@types/fs-extra": "^9.0.1",
    "@types/js-yaml": "^3.12.4",
    "@types/lunr": "^2.3.3",
    "@types/rimraf": "^3.0.0",
    "@types/spark-md5": "^3.0.2",
    "axios": "^0.19.2",
    "babel-eslint": "^10.0.1",
    "bulma": "^0.9.0",
    "commitizen": "^4.1.2",
    "dot-prop-immutable": "^2.1.0",
    "emoji-cz": "^0.3.1",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "eslint-plugin-vue-a11y": "^0.0.31",
    "fast-glob": "^3.2.2",
    "fs-extra": "^9.0.1",
    "global-jsdom": "^6.1.0",
    "js-yaml": "^3.14.0",
    "jsdom": "^16.2.2",
    "npm-run-all": "^4.1.5",
    "nuxt-property-decorator": "^2.7.2",
    "prettier": "^1.19.1",
    "query-string": "^6.13.1",
    "spark-md5": "^3.0.1",
    "stylelint": "^13.6.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylis": "^3.5.4",
    "tailwindcss": "^1.4.6",
    "ts-node": "^8.10.2",
    "typescript": "~3.8",
    "vue-tweet-embed": "^2.4.0"
  },
  "engines": {
    "node": "12.x",
    "npm": "please-use-yarn",
    "yarn": "1.x"
  },
  "config": {
    "commitizen": {
      "path": "emoji-cz"
    }
  }
}
