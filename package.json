{
  "name": "polv",
  "version": "1.0.0",
  "description": "polv&#39;s homepage",
  "author": "Pacharapol Withayasakpunt",
  "private": true,
  "scripts": {
    "predev": "node prerun.js",
    "dev": "nuxt",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "a11y": "enabler --path ./dist",
    "postinstall": "netlify-lambda install",
    "prebuild": "node prerun.js",
    "build": "run-p 'build:**'",
    "build:nuxt": "nuxt build --target static && nuxt export",
    "build:lambda": "netlify-lambda build -c ./webpack.functions.js lambda",
    "start:lambda": "netlify-lambda serve -c ./webpack.functions.js lambda"
  },
  "dependencies": {
